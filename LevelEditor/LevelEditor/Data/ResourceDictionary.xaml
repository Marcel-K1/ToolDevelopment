<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:LevelEditor">

    <system:String x:Key="MainWindowTitle">

        LevelEditor

    </system:String>

    <ImageSource x:Key="NewImage">
        \Icons\new.png
    </ImageSource>

    <ImageSource x:Key="TooltipImage">
        \Icons\tooltip.png
    </ImageSource>

    <ImageSource x:Key="HelpImage">
        \Icons\help.png
    </ImageSource>

    <SolidColorBrush x:Key="BackgroundColor1" Color="BlanchedAlmond" />

    <SolidColorBrush x:Key="BackgroundColor2" Color="AliceBlue" />


</ResourceDictionary>